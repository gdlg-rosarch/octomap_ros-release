
<stack>
  <name>octomap_ros</name>
  <version>0.2.1</version>
  <description brief="octomap_ros - conversions and wrappers to use octomap in ROS">
    octomap_ros provides conversion functions between ROS / PCL and OctoMap's native types.
    This enables a convenvient use of the octomap package in ROS.
    
  </description>
  <author email="HornungA@informatik.uni-freiburg.de">Armin Hornung</author>
  <maintainer email="HornungA@informatik.uni-freiburg.de">Armin Hornung</maintainer>
  <maintainer email="jkammerl@willowgarage.com">Julius Kammerl</maintainer>
  <license>BSD</license>
  <copyright>Armin Hornung</copyright>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/octomap_ros</url>
  
  <build_depends>cmake</build_depends>
  <build_depends>catkin</build_depends>
  <build_depends>octomap_msgs</build_depends>
  <build_depends>octomap</build_depends>
  <build_depends>perception_pcl</build_depends>
  <build_depends>geometry</build_depends>
  <build_depends>ros_comm</build_depends>
  <build_depends>common_msgs</build_depends>

  <depends>octomap_msgs</depends>
  <depends>octomap</depends>
  <depends>perception_pcl</depends>
  <depends>geometry</depends>
  <depends>ros_comm</depends>
  <depends>common_msgs</depends>

</stack>
